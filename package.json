{
	"name": "viz-fsm-viewer",
	"displayName": "Viz FSM Viewer",
	"description": "A VS Code extension to view fsm-as-promised styled finite state machine via Graphviz lib",
	"version": "0.0.1",
	"publisher": "ThankCreate",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld",
		"onCommand:vizFsmViewer.open"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "vizFsmViewer.open",
				"title": "FsmView: Open"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@babel/types": "^7.5.5",
		"@types/ejs": "^2.6.3",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.14.13",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.0-next.0"
	},
	"dependencies": {
		"@babel/core": "^7.4.3",
		"@babel/generator": "^7.4.0",
		"@types/babel__core": "^7.1.2",
		"ejs": "^2.6.2"
	}
}
